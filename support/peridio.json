{
    "version": 1,
    "device_api": {
      "certificate_path": "/etc/peridiod/peridio-cert.pem",
      "url": "device.cremini.peridio.com",
      "verify": true
    },
    "fwup": {
      "devpath": "/dev/null",
      "public_keys": []
    },
    "remote_shell": true,
    "node": {
      "key_pair_source": "env",
      "key_pair_config": {
        "private_key": "PERIDIO_PRIVATE_KEY",
        "certificate": "PERIDIO_CERTIFICATE"
      }
    }
  }
